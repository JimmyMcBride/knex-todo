{
  "name": "knex-todo",
  "version": "1.0.0",
  "description": "knex, typescript, todo back end with jwt auth",
  "main": "src/server.ts",
  "repository": "git@github.com:JimmyMcBride/knexts.git",
  "author": "Jimmy McBride",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "ts-node src/server.ts",
    "db-remake": "psql -U postgres -f remakeDatabase.sql",
    "knex-refresh": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "total-reset": "yarn db-remake && yarn knex-refresh"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
